PROJECT_NAME           = "Fge - Felix's Game Engine"
PROJECT_NUMBER         = @PROJECT_VERSION@
OUTPUT_DIRECTORY       = @CMAKE_CURRENT_BINARY_DIR@
INPUT                  = @CMAKE_CURRENT_SOURCE_DIR@/../../../README.md @CMAKE_CURRENT_SOURCE_DIR@/../src/ @CMAKE_CURRENT_SOURCE_DIR@/../include/
FILE_PATTERNS          = *.cpp *.hpp *.md
RECURSIVE              = YES
EXTRACT_ALL            = YES
EXTRACT_STATIC         = YES
MARKDOWN_SUPPORT       = YES
AUTOLINK_SUPPORT       = YES
BUILTIN_STL_SUPPORT    = YES
USE_MDFILE_AS_MAINPAGE = @CMAKE_PROJECT_SOURCE_DIR@/../../../README.md
CLANG_ASSISTED_PARSING = YES
CLANG_DATABASE_PATH    = @CMAKE_CURRENT_BINARY_DIR@/../../../
